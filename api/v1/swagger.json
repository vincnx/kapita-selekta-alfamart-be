  "components": {
    "schemas": {
      "AuthInput": {
        "type": "object",
        "properties": {
          "username": {
            "type": "string"
          },
          "password": {
            "type": "string"
          }
        }
      },
      "MasterBankInput": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string"
          },
          "bankDesc": {
            "type": "string"
          }
        },
        "required": ["name", "bankDesc"]
      },
      "VendorDetailInput": {
        "type": "object",
        "properties": {
          "vendorName": {
            "type": "string"
          },
          "unitUsaha": {
            "type": "string"
          },
          "address": {
            "type": "string"
          },
          "country": {
            "type": "string"
          },
          "province": {
            "type": "string"
          },
          "noTelp": {
            "type": "string",
            "example": "08882345345"
          },
          "emailCompany": {
            "type": "string",
            "format": "email"
          },
          "website": {
            "type": "string",
            "format": "uri"
          },
          "noNPWP": {
            "type": "string",
            "example": "08882345345543534543"
          }
        },
        "required": [
          "vendorName",
          "unitUsaha",
          "address",
          "country",
          "province",
          "noTelp",
          "emailCompany",
          "noNPWP"
        ]
      },
      "VendorBranchOfficeInput": {
        "type": "object",
        "properties": {
          "branchName": {
            "type": "string"
          },
          "address": {
            "type": "string"
          },
          "country": {
            "type": "string"
          },
          "noTelp": {
            "type": "string",
            "example": "08882345345"
          },
          "website": {
            "type": "string",
            "format": "uri"
          },
          "email": {
            "type": "string",
            "format": "email"
          }
        },
        "required": [
          "branchName",
          "address",
          "country",
          "noTelp",
          "website",
          "email"
        ]
      },
      "VendorPicInput": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string"
          },
          "noTelp": {
            "type": "string",
            "example": "08882345345"
          },
          "email": {
            "type": "string",
            "format": "email"
          }
        },
        "required": ["name", "noTelp", "email"]
      },
      "VendorBankAccountInput": {
        "type": "object",
        "properties": {
          "bankId": {
            "type": "string",
            "example": "670b1e82d3391522cc3dc538"
          },
          "accountName": {
            "type": "string"
          },
          "accountNumber": {
            "type": "string",
            "example": "08882345345"
          }
        },
        "required": ["name", "noTelp", "email"]
      },
      "BranchOffice": {
        "type": "object",
        "properties": {
          "branchName": {
            "type": "string"
          },
          "address": {
            "type": "string"
          },
          "country": {
            "type": "string"
          },
          "noTelp": {
            "type": "string"
          },
          "website": {
            "type": "string",
            "format": "uri"
          },
          "email": {
            "type": "string",
            "format": "email"
          }
        },
        "required": ["branchName", "address", "country", "noTelp", "email"]
      },
      "AccountBank": {
        "type": "object",
        "properties": {
          "bankId": {
            "type": "string"
          },
          "bankName": {
            "type": "string"
          },
          "accountNumber": {
            "type": "string"
          },
          "accountName": {
            "type": "string"
          }
        },
        "required": ["bankId", "bankName", "accountNumber", "accountName"]
      },
      "ProductInput": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string"
          },
          "count": {
            "type": "integer"
          },
          "merk": {
            "type": "string"
          },
          "condition": {
            "type": "string",
            "enum": ["new", "second"]
          },
          "vendorId": {
            "type": "string",
            "example": "672b8b35eb845da3a5bdec1e"
          }
        }
      }
    },
    "responses": {
      "200": {
        "description": "OK"
      },
      "201": {
        "description": "Created"
      },
      "204": {
        "description": "No Content"
      },
      "404": {
        "description": "Not Found"
      },
      "409": {
        "description": "Conflict"
      },
      "422": {
        "description": "Unprocessable Entity"
      },
      "500": {
        "description": "Internal Server Error"
      }
    }
  }
}
